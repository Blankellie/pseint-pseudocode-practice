Proceso Main
	
	Definir a,b,c, resultado Como Real;
	
	Escribir "Enter the A value: ";
	Leer a;
	
	Escribir "Enter the B value: ";
	Leer b;
	
	Escribir "Enter the C value: ";
	Leer c;
	
	resultado <- (-b + rc(abs(b^2 - 4*a*c)))/(2*a); 
	
	Escribir "Your result is: ", resultado;
	
FinProceso
